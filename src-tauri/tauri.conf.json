{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "grape-planit",
  "version": "0.1.0",
  "identifier": "com.ttata.planit",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "grape-planit",
        "width": 450,
        "height": 250,
        "decorations": false,
        "alwaysOnTop": true,
        "transparent": true,
        "shadow": true,
        "resizable": false,
        "x": null,
        "y": null,
        "center": true
      },
      {
        "label": "settings",
        "title": "设置",
        "url": "/settings",
        "width": 570,
        "height": 400,
        "decorations": true,
        "alwaysOnTop": false,
        "transparent": false,
        "shadow": true,
        "center": true,
        "visible": false
      }
    ],

    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}